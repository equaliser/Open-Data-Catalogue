				</ul>
			</nav>
			
			<h1 id="pageHeader"><a href="/">Warwickshire Open Data</a></h1>
		</header>
		
		<div id="home" class="pageContent clearfix">
			<nav id="pageNav">
				<ul>
					<%= navigation [{:datasets => datasets_path}, 
									{:categories => categories_path}]%>					
				<!--	<li><a href="#">Categories</a></li>
					<li><a href="#">Tags</a></li>
					<li><a href="#">Thing</a></li>
					<li><a href="/blogposts">Blog</a></li>
					<li class="last"><a href="#">Community</a></li>-->
				</ul>
			</nav>
			<div id="mainCol">
				<% if flash[:notice] %>
					<p id="flashNotice"><%= flash[:notice] -%></p>
				<% end %>
				<% if flash[:error] %>
					<p id="flashError"><%= flash[:error] -%></p>
				<% end %>
				<%= yield %>
