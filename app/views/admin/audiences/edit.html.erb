<h1>Editing audience</h1>

<% form_for([:admin, @audience]) do |f| %>
  <%= f.error_messages %>

  <p>
    <%= f.label :name %><br />
    <%= f.text_field :name %>
  </p>
  <p>
    <%= f.submit 'Update' %>
  </p>
<% end %>

<%= link_to 'Show', [:admin, @audience] %> |
<%= link_to 'Back', admin_audiences_path %>